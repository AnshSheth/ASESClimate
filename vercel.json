{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "functions": {
    "api/*.py": {
      "runtime": "python3.9",
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/api/enhance-document",
      "dest": "/api/enhance.py"
    },
    {
      "src": "/api/download-pdf",
      "dest": "/api/download.py"
    }
  ]
} 